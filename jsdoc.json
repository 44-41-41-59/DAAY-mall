{
  "source": {
      "include": ["./src"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|./doc)"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "opts": {
      "encoding": "utf8",
      "destination": "doc/",
      "recurse": true,
      "verbose": true,
      "template":"default"
  }
}
